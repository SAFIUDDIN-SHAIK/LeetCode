# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def getDecimalValue(self, head: ListNode) -> int:
        ans=0
        while head:
            t=head.val
            head=head.next
            ans=ans<<1
            if t:
                ans+=1
            
        return ans
