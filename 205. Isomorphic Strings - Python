class Solution:
    def isIsomorphic(self, s: str, t: str) -> bool:
        d = {}
        d1 = {}
        for i in range(len(s)):
            if s[i] in d:
                if d[s[i]] != t[i]:
                    return 0
            else:
                d[s[i]] = t[i]
            if t[i] in d1:
                if d1[t[i]] != s[i]:
                    return 0
            else:
                d1[t[i]] = s[i]
        return 1
